{
	"name": "dereuromark/cakephp-cache",
	"description": "Cache plugin for CakePHP",
	"type": "cakephp-plugin",
	"license": "MIT",
	"require": {
		"php": ">=5.5",
		"cakephp/cakephp": "~3.3"
	},
	"require-dev": {
		"dereuromark/cakephp-tools": "dev-master",
		"fig-r/psr2r-sniffer": "dev-master",
		"phpunit/phpunit": "*"
	},
	"autoload": {
		"psr-4": {
			"Cache\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Cache\\Test\\": "tests",
			"TestApp\\": "tests/TestApp/src",
			"Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
		}
	}
}
